pipeline{
  agent any
  stages{
     stage("Mvn Build"){
       steps{
            sh "mvn clean package"
        }
    }
    stage("deploy-dev"){
       steps{
          sshagent(['deploy']) {
            sh "chmod 777 /home/test/.jenkins -R"
            sh "cp -rp /home/test/.jenkins/workspace/pipeline/target/*.jar /home/test/output"
            sh "scp -rp /home/test/output/*.jar test@54.67.106.24:/home/test"
          }
        }
    }
    }
}
